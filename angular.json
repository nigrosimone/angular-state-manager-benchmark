{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngrx": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ngrx",
      "sourceRoot": "projects/ngrx/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/ngrx/src/main.ts",
            "tsConfig": "projects/ngrx/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ngrx/public"
              }
            ],
            "styles": [
              "projects/ngrx/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngrx:build:production"
            },
            "development": {
              "buildTarget": "ngrx:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngrx/**/*.ts",
              "projects/ngrx/**/*.html"
            ],
            "eslintConfig": "projects/ngrx/eslint.config.js"
          }
        }
      }
    },
    "ngss": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ngss",
      "sourceRoot": "projects/ngss/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/ngss/src/main.ts",
            "tsConfig": "projects/ngss/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ngss/public"
              }
            ],
            "styles": [
              "projects/ngss/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngss:build:production"
            },
            "development": {
              "buildTarget": "ngss:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngss/**/*.ts",
              "projects/ngss/**/*.html"
            ],
            "eslintConfig": "projects/ngss/eslint.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "angular-eslint"
    ]
  }
}

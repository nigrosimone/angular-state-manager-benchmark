{
  "name": "angular-benchmark",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:ngrx": "ng serve todos-ngrx --port 4200",
    "start:ngss": "ng serve todos-ngss --port 4201",
    "cypress:open": "cypress open",
    "benchmark": "npx concurrently -k \"npm run start:ngrx\" \"npm run start:ngss\" \"npx wait-on http://localhost:4200 http://localhost:4201 && npx cypress run\"",
    "benchmark:open": "npx concurrently -k \"npm run start:ngrx\" \"npm run start:ngss\" \"npx wait-on http://localhost:4200 http://localhost:4201 && npx cypress open\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@ngrx/store": "^20.1.0",
    "ng-simple-state": "^20.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.5.0",
    "typescript": "~5.9.2"
  }
}

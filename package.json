{
  "name": "angular-benchmark",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start:ngrx": "ng serve ngrx --port 4200",
    "start:ngss": "ng serve ngss --port 4201",
    "start:elf": "ng serve elf --port 4202",
    "start:all": "npx concurrently -k \"npm run start:ngrx\" \"npm run start:ngss\" \"npm run start:elf\"",
    "build:ngrx": "ng build ngrx",
    "build:ngss": "ng build ngss",
    "build:elf": "ng build elf",
    "build:all": "npm run build:ngrx && npm run build:ngss && npm run build:elf",
    "serve:ngrx": "http-server ./dist/ngrx/browser -p 4200",
    "serve:ngss": "http-server ./dist/ngss/browser -p 4201",
    "serve:elf": "http-server ./dist/elf/browser -p 4202",
    "serve:all": "npx concurrently -k \"npm run serve:ngrx\" \"npm run serve:ngss\"",
    "wait-on": "npx wait-on http://localhost:4200 http://localhost:4201 http://localhost:4202",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "benchmark:dev": "npx concurrently -k \"npm run start:all\" \"npm run wait-on && npx cypress run\"",
    "benchmark:dev:open": "npx concurrently -k \"npm run start:all\" \"npm run wait-on && npx cypress open\"",
    "benchmark:prod": "npm run build:all && npx concurrently -k \"npm run serve:all\" \"npm run wait-on && npx cypress run\"",
    "benchmark:prod:open": "npm run build:all && npx concurrently -k \"npm run serve:all\" \"npm run wait-on && npx cypress open\"",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@ngneat/elf": "^2.5.1",
    "@ngrx/store": "^20.1.0",
    "ng-simple-state": "^20.1.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "angular-eslint": "20.5.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "eslint-plugin-cypress": "^5.2.0",
    "http-server": "^14.1.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.1"
  }
}